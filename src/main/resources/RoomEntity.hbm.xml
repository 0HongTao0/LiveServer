<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hongtao.live.dao.entity.RoomEntity" table="room" schema="Live">
        <id name="roomId" column="roomId"/>
        <property name="num" column="num"/>
        <property name="userId" column="userId"/>
        <property name="url" column="url"/>
        <property name="living" column="living"/>
        <property name="roomName" column="roomName"/>
        <property name="roomIntroduction" column="roomIntroduction"/>
    </class>
</hibernate-mapping>